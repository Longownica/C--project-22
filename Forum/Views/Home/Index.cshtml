
@model IEnumerable<Forum.Entities.Post>

<head>
    <link href="@Url.Content("~/Views/Home/Index.cshtml.css")" rel="stylesheet" type="text/css" />
    <link rel ="stylesheet" href="~/@(nameof(Forum)).styles.css" />
    <link rel="stylesheet" href="~/Views/Home/Index.cshtml.css" />


    <script type="text/javascript">
        async function deletePost(id) {
            const response = await fetch(`/api/forum/${id}`, {
                method: 'DELETE'
            });
            //if (response.ok == true) alert("Usunieto post");
            window.location = "/";
        }
    </script>
</head>
<style>
    @using System.IO
    @using Forum.Helpers
    @Html.Raw(File.ReadAllText(MyServer.MapPath("Views/Home/Index.cshtml.css")))
</style>

    @{
    ViewBag.Title = "Index";
    }
    
<body style="background-color: #a9c681 ">
    <div class="start">
        <a href="/"><h1>SUPER FORUM</h1></a>
        <h3>dla super uzytkownikow</h3>

        <h1>Nowy post:</h1>

        <form method="post" enctype="multipart/form-data">
            <label for="fname">Temat:</label>
            <input type="text" id="fname" name="topic" asp-for="topic"><br><br>

            <label for="lname">Nick:</label>
            <input type="text" id="lname" name="nickname" asp-for="nickname"><br><br>

            <label for="lname">Odpowiedz do:</label>
            <input type="number" id="lname" name="answerTo" asp-for="answerTo"><br><br>

            <label for="lname">Tresc:</label>
            <textarea name="body" cols="35" rows="4" placeholder=" " asp-for="body"></textarea><br><br>

            <label for="file">Plik:</label>
            <input id="image" class="upload" type="file" name="file" asp-for="file" accept="image/png, image/jpg" runat="server"><br><br>

            <input type="submit" value="Wyslij">
        </form>
    </div>

    @{
        var originalPost = new List<Forum.Entities.Post>();
        foreach (var item in Model)
        {
            if (item.answerTo < 1) originalPost.Add(item);
        }
        var descendingList = originalPost.OrderByDescending(x => x.date).ToList();

    }


    @foreach (var item in descendingList)
    {
        <div class="thread">
            <hr>
            <div class="opContainer">
                <span class="id" style="color: #406010; font-weight:bold">ID: @item.Id</span>
                <span class="topic" style="color: #406010; font-weight: bold">@item.topic</span>
                <span class="nickname" style="color: #406010; font-weight: bold ">@item.nickname</span>
                <span class="date" style="color: #406010; font-weight: bold">@item.date</span>
                <a asp-action="Delete" asp-route-id="@item.Id" id="@item.Id" onClick="deletePost(this.id)" style="color:blue">Delete</a>
            </div>
            <br>
            <div class="image">
            <a class="image">
                @{
                    string img = "";
                    try
                    {
                        img = Convert.ToBase64String(item.image);
                    }
                    catch { }
                }
                <img src="data:image/jpeg;base64, @img">
            </a>
            </div>
            <div class="block">
                <blockqoute class="postMessage">
                    <span class="body" style="color: #2f470c">@item.body</span>
                </blockqoute>
            </div>
            <hr>
            
        </div>


        var answers = new List<Forum.Entities.Post>();
        foreach (var ans in Model)
        {
            if (ans.answerTo == item.Id) answers.Add(ans);
        }

        foreach (var ans in answers)
        {
            <div class="ansThread">
                <div class="ansContainer">
                    <span class="id" style="color: #406010; font-weight: bold";>ID: @ans.Id</span>
                    <span class="topic" style="color: #406010; font-weight: bold">@ans.topic</span>
                    <span class="nickname" style="color: #406010; font-weight: bold">@ans.nickname</span>
                    <span class="date" style="color: #406010; font-weight: bold">@ans.date</span>
                    <a asp-action="Delete" asp-route-id="@ans.Id" id="@ans.Id" onClick="deletePost(this.id)" style="color:blue">Delete</a>
                </div>
                <br>
               <div class="ansbody">
                <div class="image">
                <a class="image">

                    @{
                        string img2 = "";
                        try
                        {
                            img2 = Convert.ToBase64String(ans.image);
                        }
                        catch { }
                    }
                    <img src="data:image/jpeg;base64, @img2">
                </a>
                </div>
                <div class="block">
                <blockqoute class="postMessage">
                    <span class="body" style="color: #2f470c">@ans.body</span>
                </blockqoute>
                </div>
               </div>
            </div>
            <br>
        }
        <br>
    }
</body>